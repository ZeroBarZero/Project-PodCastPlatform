<!--- episode list -->
<div class="podcast-page">

<div id="wrapper">

<div data-announcement-top-bar="/ajax/announcement"></div>
<% for (let post of posts) { %>
<div class="podcast-episodes golden-theme show-scroll-arrow animate" data-song-index="0" style="background-image: url('/')">
	<a id="season_two"></a>
	<div class="inner-section">
		<div class="episode-details">
			<div class="illustration small-image">
				<div class="illustration-sleeve">
				</div>
			</div>
			<div class="episode-details-wrapper new-episode">
				<div class="heading"> <%= post.part %> - <span><%= podCast %></span> </div>
				<h3><%=post.title %></h3>
        	<div id="audio-wrapper-second-first-mobile" class="audio-outer-wrapper">
					</div>
          <p><%=post.description %></p>
				<div class="control-wrapper">
                    <div id="audio-wrapper-second-first-tablet" class="audio-outer-wrapper">
						<div class="audio-wrapper show" data-audio-bar="">
						  <audio class="audio-basic-player" controls="">
							    <source src="https://s3.ap-northeast-2.amazonaws.com/noside-project-cdn/<%= post.url %>" type="audio/mpeg">
						    </audio>
						  <div class="audio-controls">
								<span class="button amplitude-play-pause amplitude-paused" amplitude-song-index="0"></span>
								 <div class="progress-bar">
									<input type="range" id="amplitude-song-slider-0" class="amplitude-song-slider" amplitude-song-index="0" step=".1">
									<progress id="song-played-progress-0" amplitude-song-index="0" class="amplitude-song-played-progress" value="0"></progress>
									<div class="playbutton amplitude-play" amplitude-song-index="0">LISTEN NOW</div>
								</div>
								<span class="amplitude-time-remaining" amplitude-song-index="0">24:20 <!-- ejs구문 audio-time -->
							</span>
								<span class="amplitude-mute amplitude-not-muted"></span>
							</div>
					    </div>
				    </div>
					<a class="link-button" target="_blank" href="/">

					</a>
				</div>
			</div>
		</div>
	</div>
</div>
<% }; %>

